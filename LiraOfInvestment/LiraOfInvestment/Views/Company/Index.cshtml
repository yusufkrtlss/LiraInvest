    <script>
       
        $(document).ready(function () {
            $.ajax({
                type: "Get",
                url: '@Url.Action("GetChartData","Company",new RouteValueDictionary(new {id=Model.Id}))',
                contentType: "application/json",
                dataType: "json",
                success: function (response) {
                    var data = response.close;
                    var data2 = response.timestamp;
                    var ctx = document.getElementById("myChart").getContext('2d');

                    var myChart = new Chart(ctx, {
                        type: 'line',
                        
                                data: {
                            labels: data2,
                            datasets: [{
                                label: 'close',
                                data: data,
                                borderWidth: 5,
                                radius: 2,
                                backgroundColor: 'rgb(151, 198, 131)',
                                borderColor: 'rgb(151, 198, 131)',

                                tension: 0.3
                            }
                            ]
                        },
                        options: {

                            responsive: true,
                            interaction: {
                                intersect: false
                            },
                            plugins: {
                                legend: false,
                                zoom: {
                                    pan: {
                                        enabled: true,
                                        mode: 'xy',
                                        modifierKey: 'shift',
                                        threshold: 10
                                    },
                                    zoom: {
                                        mode: 'x',
                                        wheel: {
                                            enabled: true,
                                            //speed:0.1,
                                            //modifierKey:'alt',

                                        },
                                        drag: {
                                            enabled: true,
                                            threshold: 100
                                        },
                                        pinch: {
                                            enabled: true
                                        }
                                    }
                                }
                            },
                            title: {
                                display: true,
                                text: '2 Year Monthly Chart'
                            },
                            scales: {

                                y: {
                                    beginAtZero: true
                                }
                            }
                        },
                    });
                },
                error: function (response) {
                    alert(response.responseText);
                    console.log("This is ERROR line");
                }
            });
            resetZoomBtn = (chart) => {
                console.log("merbah")
                chart.resetZoom()
            };

        });

    </script>
